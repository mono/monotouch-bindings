BTOUCH=/Developer/MonoTouch/usr/bin/btouch
SMCS=/Developer/MonoTouch/usr/bin/smcs

all: GoogleMaps.dll

GoogleMaps-iOS-1.1.0.zip:
	curl -O https://dl.google.com/geosdk/GoogleMaps-iOS-1.1.0.zip > $@
	unzip $@

GoogleMaps: GoogleMaps-iOS-1.1.0.zip
	cp GoogleMaps-iOS-1.1.0/GoogleMaps.framework/Versions/A/GoogleMaps GoogleMaps

Resources/GoogleMaps.bundle: GoogleMaps-iOS-1.1.0.zip GoogleMaps
	mkdir Resources
	cp -R GoogleMaps-iOS-1.1.0/GoogleMaps.framework/Versions/A/Resources/* Resources/GoogleMaps.bundle

GoogleMaps.dll: Makefile GoogleMaps.linkwith.cs ApiDefinition.cs StructsAndEnums.cs extras.cs GoogleMaps Resources/GoogleMaps.bundle
	-mkdir -p ios
	$(BTOUCH) -unsafe -d:DEBUG ApiDefinition.cs -s:StructsAndEnums.cs -tmpdir:ios -sourceonly:ios/sources.list
	#$(SMCS) -noconfig -debug+ -debug:full -optimize- -out:GoogleMaps.dll -resource:GoogleMaps GoogleMaps.linkwith.cs StructsAndEnums.cs extras.cs @ios/sources.list -target:library -unsafe+  -define:DEBUG -reference:/Developer/MonoTouch/usr/lib/mono/2.1/System.dll -reference:/Developer/MonoTouch/usr/lib/mono/2.1/monotouch.dll -reference:/Developer/MonoTouch/usr/lib/mono/2.1/System.Core.dll
